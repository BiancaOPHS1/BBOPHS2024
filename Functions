
speed = float(input("enter rge speed of the driver"))
if speed >= 75:
  print("fine")
elif speed >= 70 and 75:
    print("warning")
else:
  print("no action")





#sub program

def speed_driver(speed):
  if speed > 70:
    print("fine")
  else:
    print("no action")
  
  
  
#main program 

speed = float(input("enter the speed that the car was travelling at"))

print(speed_driver(speed))






